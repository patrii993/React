{
  "name": "cursor",
  "version": "1.0.0",
  "description": "Introducci√≥n a React",
  "main": "index.js",
  "scripts": {
    "start": "parcel ./src/index.html --open",
    "lint": "eslint --fix src/**/*.{js,jsx}",
    "format": "prettier --write \"*.{js,jsx,json,css}\""
  },
  "author": "Patricia F",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.3.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "parcel": "^2.8.3",
    "prettier": "^2.8.7",
    "process": "^0.11.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
